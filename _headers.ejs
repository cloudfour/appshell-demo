<%
const values = Object.values(locals);
const styles = values.filter(url => url.endsWith('.css'));
const scripts = values.filter(url => url.endsWith('.js'));
%>

# Preload links for shell styles, fonts and scripts.
/
  Link: <https://fonts.gstatic.com/s/sourcesanspro/v9/ODelI1aHBYDBqgeIAH2zlJbPFduIYtoLzwST68uhz_Y.woff2>; as=font; type="font/woff2"; crossorigin
  Link: <https://fonts.googleapis.com/css?family=Source+Sans+Pro:400>; as=style; crossorigin
  <%_ for (const url of styles) { _%>
  Link: </<%= url %>>; rel=preload; as=style
  <%_ } -%>
  <%_ for (const url of scripts) { _%>
  Link: </<%= url %>>; rel=preload; as=script
  <%_ } -%>
